{
  "name": "trillo",
  "version": "1.0.0",
  "description": "trillo app",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prifix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prifix.css",
    "compress:css": "node-sass css/style.prifix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prifix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mieskov/Trillo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mieskov/Trillo/issues"
  },
  "homepage": "https://github.com/Mieskov/Trillo#readme",
  "devDependencies": {
    "node-sass": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
